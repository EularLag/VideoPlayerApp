                <string>mov</string>
                <string>m4v</string>
                <string>avi</string>
                <string>mkv</string>
                <string>flv</string>
                <string>wmv</string>
                <string>webm</string>
            </array>
            <key>CFBundleTypeIconFile</key>
            <string></string>
            <key>CFBundleTypeName</key>
            <string>Video File</string>
            <key>CFBundleTypeRole</key>
            <string>Viewer</string>
            <key>LSHandlerRank</key>
            <string>Alternate</string>
        </dict>
    </array>
